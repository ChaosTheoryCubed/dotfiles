{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "rosewater": "#f5e0dc",
    "flamingo": "#f2cdcd",
    "pink": "#f5c2e7",
    "muave": "#cba6f7",
    "red": "#f38ba8",
    "maroon": "#eba0ac",
    "peach": "#fab387",
    "yellow": "#f9e2af",
    "green": "#a6e3a1",
    "teal": "#94e2d5",
    "sky": "#89dceb",
    "sapphire": "#74c7ec",
    "blue": "#89b4fa",
    "lavender": "#b4befe",
    "text": "#cdd6f4",
    "subtext1": "#bac2de",
    "subtext0": "#a6adc8",
    "overlay2": "#9399b2",
    "overlay1": "#7f849c",
    "overlay0": "#6c7086",
    "surface2": "#585b70",
    "surface1": "#45475a",
    "surface0": "#313244",
    "base": "#1e1e2e",
    "mantle": "#181825",
    "crust": "#11111b"
  },
  "secondary_prompt": {
    "template": "<p:lavender,transparent>\ue0b6</><,p:lavender> > </><p:lavender,transparent>\ue0b0</> ",
    "foreground": "p:crust",
    "background": "transparent"
  },
  "transient_prompt": {
    "template": "<p:lavender,transparent>\ue0b6</><,p:lavender> {{ .Folder }} </><p:lavender,transparent>\ue0b0</> ",
    "foreground": "p:crust",
    "background": "transparent"
  },
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "enable_cursor_positioning": true,
  "upgrade": {
    "source": "cdn",
    "interval": "168h",
    "auto": false,
    "notice": false
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "template": " {{ if .SSHSession }}\ueba9 {{ end }}{{ .UserName }} ",
          "foreground": "p:crust",
          "background": "p:lavender",
          "type": "session",
          "style": "diamond"
        },
        {
          "properties": {
            "style": "folder"
          },
          "template": " \uea83 {{ path .Path .Location }} ",
          "foreground": "p:crust",
          "powerline_symbol": "\ue0b0",
          "background": "p:muave",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_template": "{{ trunc 25 .Branch }}",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " {{ if .UpstreamURL }}{{ url .UpstreamIcon .UpstreamURL }} {{ end }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
          "foreground": "p:crust",
          "powerline_symbol": "\ue0b0",
          "background": "p:green",
          "type": "git",
          "style": "powerline",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:crust{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:crust{{ end }}",
            "{{ if gt .Ahead 0 }}p:crust{{ end }}"
          ],
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:lavender{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:red{{ end }}",
            "{{ if gt .Ahead 0 }}p:lavender{{ end }}",
            "{{ if gt .Behind 0 }}p:muave{{ end }}"
          ]
        },
        {
          "template": " \uf0e7 ",
          "foreground": "p:crust",
          "powerline_symbol": "\ue0b0",
          "background": "p:lavender",
          "type": "root",
          "style": "powerline"
        },
        {
          "properties": {
            "always_enabled": true
          },
          "leading_diamond": "<transparent,background>\ue0b0</>",
          "trailing_diamond": "\ue0b4",
          "template": " {{ if gt .Code 0 }}\uf00d{{ else }}\uf00c{{ end }} ",
          "foreground": "p:crust",
          "background": "p:blue",
          "type": "status",
          "style": "diamond",
          "background_templates": ["{{ if gt .Code 0 }}p:red{{ end }}"],
          "foreground_templates": ["{{ if gt .Code 0 }}p:text{{ end }}"]
        }
      ]
    },
    {
      "type": "rprompt",
      "segments": [
        {
          "properties": {
            "display_mode": "files",
            "fetch_package_manager": false,
            "home_enabled": false
          },
          "template": "\ue718 ",
          "foreground": "p:green",
          "background": "transparent",
          "type": "node",
          "style": "plain"
        },
        {
          "properties": {
            "fetch_version": false
          },
          "template": "\ue626 ",
          "foreground": "p:blue",
          "background": "transparent",
          "type": "go",
          "style": "plain"
        },
        {
          "properties": {
            "display_mode": "files",
            "fetch_version": false,
            "fetch_virtual_env": false
          },
          "template": "\ue235 ",
          "foreground": "p:green",
          "background": "transparent",
          "type": "python",
          "style": "plain"
        },
        {
          "template": "in <p:blue><b>{{ .Name }}</b></> ",
          "foreground": "p:text",
          "background": "transparent",
          "type": "shell",
          "style": "plain"
        },
        {
          "template": "at <p:blue><b>{{ .CurrentDate | date \"15:04:05\" }}</b></>",
          "foreground": "p:text",
          "background": "transparent",
          "type": "time",
          "style": "plain"
        }
      ]
    }
  ],
  "tooltips": [
    {
      "properties": {
        "display_default": true
      },
      "leading_diamond": "\ue0b0",
      "trailing_diamond": "\ue0b4",
      "template": " \ue7ad {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} ",
      "foreground": "p:crust",
      "background": "p:peach",
      "type": "aws",
      "style": "diamond",
      "tips": ["aws"]
    },
    {
      "properties": {
        "display_default": true
      },
      "leading_diamond": "\ue0b0",
      "trailing_diamond": "\ue0b4",
      "template": " \uebd8 {{ .Name }} ",
      "foreground": "p:crust",
      "background": "p:blue",
      "type": "az",
      "style": "diamond",
      "tips": ["az"]
    }
  ],
  "version": 3,
  "final_space": true
}
